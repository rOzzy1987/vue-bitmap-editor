{
  "name": "@rozzy/vue-bitmap-editor",
  "version": "0.1.9",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
